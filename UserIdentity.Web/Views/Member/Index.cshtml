@model UserViewModel


@{
    ViewData["Title"] = "Index";
}

<h1>Kullanıcı Bilgileri</h1>

<table>
    <thead>
        <tr>
            <th>Kullanıcı Adı</th>
            <th>Email</th>
            <th>Telefon No</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td>@Model.UserName</td>
            <td>@Model.Email</td>
            <td>@Model.PhoneNumber</td>
        </tr>
    </tbody>
</table>
